<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_product_product" model="ir.sequence">
            <field name="name">Product Product</field>
            <field name="code">product.product</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_request" model="ir.sequence">
            <field name="name">Purchase Request</field>
            <field name="code">purchase.request</field>
            <field name="prefix">PR</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Customer In -->
        <record id="seq_sale_payment_register_invoice" model="ir.sequence">
          <field name="name">Sale payment register invoice sequence</field>
          <field name="code">sale.payment.register.invoice</field>
          <field name="prefix">CI</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_payment_register_refund" model="ir.sequence">
          <field name="name">Sale payment register refund sequence</field>
          <field name="code">sale.payment.register.refund</field>
          <field name="prefix">CO</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_payment_register_transfer" model="ir.sequence">
          <field name="name">Sale payment register transfer sequence</field>
          <field name="code">sale.payment.register.transfer</field>
          <field name="prefix">CT</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>


        <!--- Partner Out -->
        <record id="seq_purchase_payment_register_invoice" model="ir.sequence">
          <field name="name">Purchase payment register invoice sequence</field>
          <field name="code">purchase.payment.register.invoice</field>
          <field name="prefix">PO</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_payment_register_refund" model="ir.sequence">
          <field name="name">Purchase payment register refund sequence</field>
          <field name="code">purchase.payment.register.refund</field>
          <field name="prefix">PI</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_payment_register_transfer" model="ir.sequence">
          <field name="name">Purchase payment register transfer sequence</field>
          <field name="code">purchase.payment.register.transfer</field>
          <field name="prefix">PT</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
