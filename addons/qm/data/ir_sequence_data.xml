<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_product_product" model="ir.sequence">
            <field name="name">Product Product</field>
            <field name="code">product.product</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_request" model="ir.sequence">
            <field name="name">Purchase Request</field>
            <field name="code">purchase.request</field>
            <field name="prefix">PR</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 到款单 -->
        <record id="seq_sale_payment_register" model="ir.sequence">
          <field name="name">Sale payment register sequence</field>
          <field name="code">sale.payment.register</field>
          <field name="prefix">DK</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>

        <record id="seq_payment_register_transfer" model="ir.sequence">
          <field name="name">Payment register transfer sequence</field>
          <field name="code">payment.register.transfer</field>
          <field name="prefix">T</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>


        <!-- 请款单 -->
        <record id="seq_purchase_payment_register" model="ir.sequence">
          <field name="name">Purchase payment register sequence</field>
          <field name="code">purchase.payment.register</field>
          <field name="prefix">QK</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>


        <record id="seq_account_move_out_receipt" model="ir.sequence">
          <field name="name">QM Account Move Out Receipt</field>
          <field name="code">qm.account.move.out.receipt</field>
          <field name="prefix">KP</field>
          <field name="padding">8</field>
          <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_rma" model="ir.sequence">
            <field name="name">QM Sale RMA</field>
            <field name="code">qm.sale.rma</field>
            <field name="prefix">RMA</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
